<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxdemo.ListViewController">
   <children>
      <ListView fx:id="listView" layoutX="150.0" layoutY="24.0" prefHeight="200.0" prefWidth="300.0" />
      <Button layoutX="191.0" layoutY="309.0" mnemonicParsing="false" onAction="#addBtn" prefWidth="100.0" text="Add" />
      <Button layoutX="327.0" layoutY="309.0" mnemonicParsing="false" onAction="#deleteBtn" prefWidth="100.0" text="Delete" />
      <TextField fx:id="textField" layoutX="235.0" layoutY="254.0" promptText="Enter Brandname" />
   </children>
</AnchorPane>
