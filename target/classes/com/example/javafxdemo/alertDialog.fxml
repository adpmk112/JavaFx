<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="194.0" prefWidth="283.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxdemo.AlertDialogController">
   <children>
      <Label layoutX="49.0" layoutY="38.0" text="Do you want to close?">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button layoutX="49.0" layoutY="111.0" mnemonicParsing="false" onAction="#onYesBtnClicked" prefWidth="70.0" text="Yes" />
      <Button layoutX="165.0" layoutY="111.0" mnemonicParsing="false" onAction="#onNoBtnClicked" prefWidth="70.0" text="No" />
   </children>
</AnchorPane>
